{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["version", "darklist"],
  "properties": {
    "version": {
      "type": "string"
    },
    "darklist": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["address", "comment"],
        "properties": {
          "address": {
            "type": "string"
          },
          "comment": {
            "type": "string"
          }
        }
      }
    }
  }
}
